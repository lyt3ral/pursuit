{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "scraper",
	"main": "src/index.ts",
	"compatibility_date": "2025-08-12",
	"observability": {
		"enabled": true
	},
	"triggers": {
		"crons": ["0 */12 * * *"]
	},
	"queues": {
		"producers": [
			{
				"queue": "url-queue",
				"binding": "URL_QUEUE"
			},
			{
				"queue": "job-queue",
				"binding": "JOB_QUEUE"
			}
		],
		"consumers": [
			{
				"queue": "url-queue"
			},
			{
				"queue": "job-queue"
			}
		]
	}
}
